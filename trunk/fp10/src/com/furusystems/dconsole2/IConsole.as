package com.furusystems.dconsole2 {	import com.furusystems.dconsole2.core.gui.debugdraw.DebugDraw;	import com.furusystems.dconsole2.core.gui.maindisplay.ConsoleView;	import flash.events.Event;		/**	 * @author Andreas Roenning	 */	public interface IConsole 	{		function get view():ConsoleView;		function show():void;		function hide():void;		function toggleDisplay():void;		function executeStatement(statement:String, echo:Boolean = false):*;		function select(target:*):void;		function get visible():Boolean;		function set visible(b:Boolean):void;		function set defaultInputCallback(f:Function):void;		function get defaultInputCallback():Function;		function print(str:String, type:String = "Info", tag:String = ""):void;		function changeKeyboardShortcut(keystroke:uint, modifier:uint):void;		function onEvent(e:Event):void;		function clear():void;		function createCommand(keyword:String, func:Function, commandGroup:String = "Application", helpText:String = ""):void;				function removeCommand(triggerPhrase:String):void;		function setHeaderText(title:String):void;		function setOverrideCallback(callback:Function):void;		function clearOverrideCallback():void;				function lockOutput():void;		function unlockOutput():void;				function get debugDraw():DebugDraw;			}	}